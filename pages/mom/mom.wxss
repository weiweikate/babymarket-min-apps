/* mom.wxss */
@import '/pages/mom/tab-layout/tab-layout.wxss';

.coupon-box {
  display: flex;
  flex-direction: column;
  height: 100%;
}

.top{
    position: fixed;
    top: 0;
    z-index: 20;
}

.swiper-view {
  margin-top: 96rpx;
  height: 100%;
}

.swiper-view .swiper-container {
  height: 100%;
}

.swiper-view .swiper-container .scroll {
  height: 100%;
}

.cart-no-data {
  justify-content: center;
  height: 100%;
  width: 100%;
  display: flex;
  align-items: center;
}